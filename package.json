{
  "name": "app-code",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:unit": "jest --selectProjects=unit",
    "test:integration": "jest --selectProjects=integration",
    "test:component": "jest --selectProjects=component",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:component && npm run test:e2e",
    "audit": "npm audit --production",
    "build:production": "npm run validate:env && npm run prisma:generate && npm run lint && npm run build && npm run postbuild",
    "build:staging": "npm run prisma:generate && npm run build",
    "build:preview": "npm run prisma:generate && npm run build",
    "build:branch": "npm run prisma:generate && npm run build",
    "postbuild": "next-sitemap && npm run compress:static",
    "compress:static": "find .next/static -type f \\( -name '*.js' -o -name '*.css' \\) -exec gzip -9 -k {} \\;",
    "validate:env": "node scripts/validate-env.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "deploy:production": "npm run build:production && netlify deploy --prod",
    "deploy:staging": "npm run build:staging && netlify deploy --alias staging",
    "predeploy": "npm run test:ci && npm run validate:build",
    "validate:build": "node scripts/validate-build.js",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^6.0.1",
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@hookform/resolvers": "^5.2.1",
    "@netlify/functions": "^4.2.1",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-tabs": "^1.1.12",
    "@react-google-maps/api": "^2.20.7",
    "@sentry/serverless": "^7.120.4",
    "@supabase/supabase-js": "^2.53.0",
    "@types/dompurify": "^3.0.5",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "csrf-csrf": "^4.0.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "express-rate-limit": "^8.0.1",
    "google-auth-library": "^10.2.0",
    "googleapis": "^154.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.534.0",
    "next": "15.4.5",
    "next-sitemap": "^4.2.3",
    "node-cron": "^3.0.3",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "redis": "^5.7.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.14",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^3.4.17",
    "postcss": "^8.4.49",
    "autoprefixer": "^10.4.20",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "jest-environment-jsdom": "^30.0.5",
    "jest-mock-extended": "^4.0.0",
    "identity-obj-proxy": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^3.0.1",
    "msw": "^2.10.4",
    "supertest": "^7.1.4",
    "typescript": "^5"
  }
}
